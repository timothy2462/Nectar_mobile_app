{
  "expo": {
    "name": "Nectar",
    "slug": "Nectar",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splach-logo.png",
        "backgroundColor": "#53B175"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splach-logo.png"
    },
    "splash": {
      "image": "./assets/images/splach-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#53B175"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splach-logo.png",
          "imageWidth": 150,
          "resizeMode": "cover",
          "backgroundColor": "#53B175"
        }
      ]
   
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
